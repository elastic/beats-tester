# {{ ansible_managed }}

metricbeat.max_start_delay: 0

metricbeat.config.modules:
  path: ${path.config}/modules.d/*.yml

output.file:
  path: '${path.logs}'
  filename: output.json

logging:
  level: info
  json: true
  to_files: true
  files:
    name: "{{ beat_name }}.json"
  metrics:
    enabled: true

http.host: localhost