# {{ ansible_managed }}

packetbeat.interfaces.device: {{ packetbeat_device }}

packetbeat.protocols:
- type: http
  ports: [80]

output.file:
  path: '${path.logs}'
  filename: output.json

fields:
  hello: ${ES_PWD}

logging:
  level: info
  json: true
  files:
    name: {{ beat_name }}.json

http.host: localhost
